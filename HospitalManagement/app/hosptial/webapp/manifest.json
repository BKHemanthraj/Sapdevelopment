{
  "_version": "1.76.0",
  "sap.app": {
    "id": "hosptial",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.19.6",
      "toolsId": "9a8c1341-c4d3-4b7c-a56d-f1bcb820b289"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/my/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "hosptial.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "PatientsList",
          "target": "PatientsList"
        },
        {
          "pattern": "Patients({key}):?query:",
          "name": "PatientsObjectPage",
          "target": "PatientsObjectPage"
        },
        {
          "pattern": "Patients({key})/patientsTovisits({key2}):?query:",
          "name": "VisitsObjectPage",
          "target": "VisitsObjectPage"
        }
      ],
      "targets": {
        "PatientsList": {
          "type": "Component",
          "id": "PatientsList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Patients",
              "variantManagement": "Page",
              "navigation": {
                "Patients": {
                  "detail": {
                    "route": "PatientsObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  },
                  "actions": {
                    "ButtonAction": {
                      "press": "hosptial.ext.controller.HandleCustom.CustomDetails",
                      "visible": true,
                      "enabled": true,
                      "requiresSelection": false,
                      "text": "Custom Button"
                    }
                  }
                }
              },
              "defaultTemplateAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#tableView"
            }
          }
        },
        "PatientsObjectPage": {
          "type": "Component",
          "id": "PatientsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Patients",
              "navigation": {
                "patientsTovisits": {
                  "detail": {
                    "route": "VisitsObjectPage"
                  }
                }
              },
              "content": {
                "header": {
                  "actions": {
                    "Total": {
                      "press": "hosptial.ext.controller.TotalSum.SumCalculation",
                      "visible": true,
                      "enabled": true,
                      "text": "Total",
                      "position": {
                        "placement": "Before",
                        "anchor": "DeleteAction"
                      }
                    },
                    "FrontTotal": {
                      "press": "hosptial.ext.controller.FrontendTotal.FrontendTotal",
                      "visible": true,
                      "enabled": true,
                      "text": "FrontendTotal",
                      "position": {
                        "placement": "After",
                        "anchor": "Total"
                      }
                    }
                  }
                },
                "body": {
                  "sections": {
                    "Fragments1": {
                      "template": "hosptial.ext.fragment.Fragments1",
                      "position": {
                        "placement": "After",
                        "anchor": "Vistor"
                      },
                      "title": "New Section"
                    },
                    "DailogBox": {
                      "template": "hosptial.ext.fragment.DailogBox",
                      "position": {
                        "placement": "After",
                        "anchor": "Fragments1"
                      },
                      "title": "Dailog"
                    }
                  }
                }
              }
            }
          }
        },
        "VisitsObjectPage": {
          "type": "Component",
          "id": "VisitsObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Patients/patientsTovisits"
            }
          }
        }
      }
    },
    "extends": {
      "extensions": {
        "sap.ui.controllerExtensions": {
          "sap.fe.templates.ObjectPage.ObjectPageController#hosptial::PatientsObjectPage": {
            "controllerName": "hosptial.ext.controller.OnIt"
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
